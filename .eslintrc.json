{
  "env": {
    "node": true,
    "jest": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
  "parserOptions": { "ecmaVersion": 2021 },
  "rules": {
    "no-console": "off",
    "prefer-const": "error",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  }
}
